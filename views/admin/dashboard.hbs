    <script>
        document.addEventListener("DOMContentLoaded", function () {
            fetch("/dashboard-data")
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok');
                    }
                    return response.json();
                })
                .then(data => {
                    const { lineChartLabels, lineChartData, ordersCount, paymentGraphData, totalOrders, totalProducts, totalRevenue } = data

                    console.log(data)

                    roundChartLoading(ordersCount)
                    lineChartLoading(lineChartLabels, lineChartData)
                    createOrdersElement(ordersCount)
                    paymentMethodChart(paymentGraphData)
                    totalOrdersRender(totalOrders)
                    totalProductsRender(totalProducts)
                    totalRevenueRender(totalRevenue)
                })
                .catch(error => {
                    console.error('Error fetching data:', error);
                });
        });
        function totalProductsRender(totalProducts) {
            const container = document.getElementById("total-products");
            const totalOrdersSpan = document.createElement("span")
            totalOrdersSpan.classList.add("fs-6", "fw-bold", "me-2")
            totalOrdersSpan.textContent = totalProducts
            container.appendChild(totalOrdersSpan)
        }


        function totalOrdersRender(count) {
            const container = document.getElementById("total-orders");
            const totalOrdersSpan = document.createElement("span")
            totalOrdersSpan.classList.add("fs-6", "fw-bold", "me-2")
            totalOrdersSpan.textContent = count
            container.appendChild(totalOrdersSpan)
        }

        function totalRevenueRender(count) {
            const container = document.getElementById("total-revenue");
            const totalRevenuesSpan = document.createElement("span")
            totalRevenuesSpan.classList.add("fs-6", "fw-bold", "me-2")
            totalRevenuesSpan.textContent = count
            container.appendChild(totalRevenuesSpan)
        }

        function roundChartLoading(ordersCount) {
            const totalNumberOfDeliveredOrders = ordersCount?.delivered || 0;
            const cancelledOrdersCount = ordersCount?.cancelled || 0;
            const shippedOrdersCount = ordersCount?.shipped || 0;
            const pendingOrdersCount = ordersCount?.pending || 0;

            // Create the pie chart
            const ctx = document.getElementById('myPieChart').getContext('2d');
            const myPieChart = new Chart(ctx, {
                type: 'pie',
                data: {
                    labels: ['Delivered', 'Cancelled', 'Shipped', 'Pending'],
                    datasets: [{
                        label: 'Order Status',
                        data: [totalNumberOfDeliveredOrders, cancelledOrdersCount, shippedOrdersCount, pendingOrdersCount],
                        backgroundColor: [
                            'rgb(255, 99, 132)',
                            'rgb(54, 162, 235)',
                            'rgb(255, 205, 86)',
                            'rgb(75, 192, 192)'
                        ],
                        hoverOffset: 4
                    }]
                },
                options: {
                    responsive: true,
                    plugins: {
                        legend: {
                            position: 'top',
                        },
                        title: {
                            display: true,
                            text: 'Order Status'
                        }
                    }
                }
            });
        }

        function lineChartLoading(lineChartLabels, lineChartData) {
            const chartData = {
                labels: lineChartLabels,
                datasets: [{
                    label: "Revenue", // This should be a string representing the label, not an array of dates
                    data: lineChartData,
                    fill: false,
                    borderColor: 'rgb(75, 192, 192)',
                    tension: 0.1
                }]
            };

            // Configure the chart
            const config = {
                type: 'line',
                data: chartData
            };

            // Get the canvas context and render the chart
            const ctx = document.getElementById('lineChart').getContext('2d');
            new Chart(ctx, config);
        }


        function createOrdersElement(ordersCount) {
            const container = document.getElementById("ordersContainer");

            // Create a div to hold the row
            const rowDiv = document.createElement("div");
            rowDiv.classList.add("row", "g-3", "mb-3", "row-cols-1", "row-cols-sm-2", "row-cols-md-2", "row-cols-lg-2", "row-cols-xl-4");

            // Loop through each type of order and create a column for it
            Object.keys(ordersCount).forEach(type => {
                const colDiv = document.createElement("div");
                colDiv.classList.add("col");

                const alertDiv = document.createElement("div");
                alertDiv.classList.add("alert", type === "delivered" ? "alert-success" : type === "cancelled" ? "alert-danger" : type === "pending" ? "alert-warning" : "alert-info", "mb-0");

                const dFlexDiv = document.createElement("div");
                dFlexDiv.classList.add("d-flex", "align-items-center");

                const avatarDiv = document.createElement("div");
                avatarDiv.classList.add("avatar", "rounded", "no-thumbnail", type === "delivered" ? "bg-success" : type === "cancelled" ? "bg-danger" : type === "pending" ? "bg-warning" : "bg-info", "text-light");
                const icon = document.createElement("i");
                icon.classList.add(type === "delivered" ? "fa-dollar" : type === "cancelled" ? "fa-credit-card" : type === "pending" ? "fa-smile-o" : "fa-shopping-bag", "fa-lg");
                avatarDiv.appendChild(icon);

                const flexFillDiv = document.createElement("div");
                flexFillDiv.classList.add("flex-fill", "ms-3", "text-truncate");
                const h6 = document.createElement("div");
                h6.classList.add("h6", "mb-0");
                h6.textContent = type.charAt(0).toUpperCase() + type.slice(1); // Capitalize the first letter
                const span = document.createElement("span");
                span.classList.add("small");
                span.textContent = ordersCount[type];

                flexFillDiv.appendChild(h6);
                flexFillDiv.appendChild(span);

                dFlexDiv.appendChild(avatarDiv);
                dFlexDiv.appendChild(flexFillDiv);

                alertDiv.appendChild(dFlexDiv);
                colDiv.appendChild(alertDiv);

                rowDiv.appendChild(colDiv);
            });

            // Append the row to the container
            container.appendChild(rowDiv);
        }

function paymentMethodChart(paymentGraphData) {
    const ctx = document.getElementById('paymentMethodChart').getContext('2d');

    // Define chart configuration
    const chartConfig = {
        type: 'doughnut',
        data: {
            labels: paymentGraphData.map((item) => item.paymentMethod),
            datasets: [{
                label: 'Payment Methods',
                data: paymentGraphData.map((item) => item.count), // Use count instead of totalRevenue
                backgroundColor: [
                    'rgb(148, 0, 211)', // Violet
                    'rgb(47, 79, 79)'   // Dark colors
                ],
                borderColor: 'rgba(255, 255, 255, 0.5)',
                borderWidth: 2
            }]
        },
        options: {
            responsive: true,
            plugins: {
                legend: {
                    position: 'top',
                },
                title: {
                    display: true,
                    text: 'Payment Method Distribution'
                }
            }
        }
    };

    // Create new chart instance
    new Chart(ctx, chartConfig);
}


